{
  "name": "demage", 
  "version-string": "1.0.0",
  "dependencies": [ 
    "sdl2",
	"sdl2-mixer",
	{
		"name": "sdl2-image",
		"default-features": false,
		"features": ["libjpeg-turbo","libwebp", "tiff"]
	},
	"sdl2-ttf",
	"spdlog",
	"glm",
	"nlohmann-json",
	{
		"name": "imgui",
		"default-features": false,
		"features": ["dx12-binding","win32-binding", "sdl2-binding", "sdl2-renderer-binding"]
	}
  ]
}